{
  "name": "@alphred/dashboard",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port 8080",
    "typecheck": "node -e \"require('fs').rmSync('.next/types', { recursive: true, force: true })\" && tsc --noEmit",
    "typecheck:test": "tsc --noEmit -p tsconfig.test.json",
    "build": "next build",
    "start": "next start --port 8080",
    "test:e2e": "env -u NO_COLOR node ./scripts/playwright-test.mjs --config ../../playwright.config.ts",
    "test:e2e:no-test-routes": "env -u NO_COLOR node ./scripts/playwright-test.mjs --config ../../playwright.no-test-routes.config.ts",
    "test:e2e:build-gate": "env -u NO_COLOR node ./scripts/playwright-test.mjs --config ../../playwright.build-gate.config.ts",
    "test:e2e:all": "pnpm test:e2e && pnpm test:e2e:no-test-routes && pnpm test:e2e:build-gate"
  },
  "dependencies": {
    "next": "^16.0.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.0"
  }
}
